package com.projeto.operadores;

public class Operador {

	/*
		OPERADORES RELACIONAIS
	=  (ATRIBUIÇÃO)
	== (IGUAL A)
	!= (DIFERENTE)
	>  (MAIOR QUE)
	<  (MENOR QUE)
	>= (MAIOR OU IGUAL)
	<= (MENOR OU IGUAL)
	
		OPERADORES LÓGICOS
	&& (E)
	|| (OU)

*/

public static void main(String[] args) {
	
	boolean igualdade;
	
	igualdade = 5 == 3;

System.out.println(igualdade);
	}

}
